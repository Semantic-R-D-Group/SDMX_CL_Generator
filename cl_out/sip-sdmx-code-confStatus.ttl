#####################################################
# CL_CONFSTATUS Scheme Definition
#####################################################

sip-sdmx-code:confStatus a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Confidentiality Status - codelist scheme"@en ;
    rdfs:label "Confidentiality Status - codelist scheme"@en ;
    skos:notation "CL_CONFSTATUS" ;
    skos:definition "A reference confStatus codelist based on SDMX:CL_CONF_STATUS(1.3)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/SDMX/CL_CONF_STATUS/1.3> ;
    rdfs:seeAlso sip-sdmx-concept:CONF_STATUS ;
    skos:related sip-sdmx-concept:CONF_STATUS ;
    rdfs:seeAlso sip-sdmx-code:ConfStatus .

sip-sdmx-code:confStatus skos:exactMatch sdmx-code:confStatus .

sip-sdmx-code:ConfStatus a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Confidentiality Status - codelist class"@en ;
    rdfs:label "Confidentiality Status - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:confStatus ;
    skos:inScheme sip-sdmx-code:confStatus .


########################################
# CL_CONFSTATUS Codes
########################################
    
sip-sdmx-code:confStatus-A a skos:Concept, sip-sdmx-code:ConfStatus;
    skos:topConceptOf sip-sdmx-code:confStatus ;
    skos:inScheme sip-sdmx-code:confStatus ;
    skos:notation "A" ;
    skos:prefLabel "Primary confidentiality due to small counts"@en ;
    rdfs:label "Primary confidentiality due to small counts"@en .

sip-sdmx-code:confStatus skos:hasTopConcept sip-sdmx-code:confStatus-A .



#####################################################
# CL_GRADE Scheme Definition
#####################################################

sip-sdmx-code:grade a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Grade - codelist scheme"@en ;
    rdfs:label "Grade - codelist scheme"@en ;
    skos:notation "CL_GRADE" ;
    skos:definition "A reference grade codelist based on UIS:CL_GRADE(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/UIS/CL_GRADE/1.0> ;
    rdfs:seeAlso sip-sdmx-code:Grade .

sip-sdmx-code:Grade a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Grade - codelist class"@en ;
    rdfs:label "Grade - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:grade ;
    skos:inScheme sip-sdmx-code:grade .



#####################################################
# CL_TIMETRANSTYPE Scheme Definition
#####################################################

sip-sdmx-code:timeTransType a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Time Transformation Type - codelist scheme"@en ;
    rdfs:label "Time Transformation Type - codelist scheme"@en ;
    skos:notation "CL_TIMETRANSTYPE" ;
    skos:definition "A reference timeTransType codelist based on SDMX:CL_TIMETRANS_TYPE(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/SDMX/CL_TIMETRANS_TYPE/1.0> ;
    skos:note "SDMX:CL_TIMETRANS_TYPE(1.0): Code list for time-related transformation types of time series."@en ;
    rdfs:seeAlso sip-sdmx-code:TimeTransType .

sip-sdmx-code:TimeTransType a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Time Transformation Type - codelist class"@en ;
    rdfs:label "Time Transformation Type - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:timeTransType ;
    skos:inScheme sip-sdmx-code:timeTransType .



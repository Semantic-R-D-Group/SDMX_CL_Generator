@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sdmx: <http://purl.org/linked-data/sdmx#> .
@prefix sdmx-concept: <http://purl.org/linked-data/sdmx/2009/concept#> .
@prefix sdmx-code: <http://purl.org/linked-data/sdmx/2009/code#> .
@prefix sip-sdmx-concept: <https://purl.semanticip.org/linked-data/sdmx/concept/> .
@prefix sip-sdmx-code: <https://purl.semanticip.org/linked-data/sdmx/code/> .
@prefix sip-sdmx: <https://purl.semanticip.org/linked-data/sdmx> .


#####################################################
# CL_DEMANDPROD Scheme Definition
#####################################################

sip-sdmx-code:demandProd a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Demand or production - codelist scheme"@en ;
    rdfs:label "Demand or production - codelist scheme"@en ;
    skos:notation "CL_DEMANDPROD" ;
    skos:definition "A reference demandProd codelist based on ESTAT:CL_DEMAND_PROD(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/ESTAT/CL_DEMAND_PROD/1.0> ;
    rdfs:seeAlso sip-sdmx-code:DemandProd .

sip-sdmx-code:DemandProd a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Demand or production - codelist class"@en ;
    rdfs:label "Demand or production - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:demandProd ;
    skos:inScheme sip-sdmx-code:demandProd .


########################################
# CL_DEMANDPROD Codes
########################################
    
sip-sdmx-code:demandProd-D a skos:Concept, sip-sdmx-code:DemandProd;
    skos:topConceptOf sip-sdmx-code:demandProd ;
    skos:inScheme sip-sdmx-code:demandProd ;
    skos:notation "D" ;
    skos:prefLabel "Demand based"@en ;
    rdfs:label "Demand based"@en .

sip-sdmx-code:demandProd skos:hasTopConcept sip-sdmx-code:demandProd-D .


sip-sdmx-code:demandProd-P a skos:Concept, sip-sdmx-code:DemandProd;
    skos:topConceptOf sip-sdmx-code:demandProd ;
    skos:inScheme sip-sdmx-code:demandProd ;
    skos:notation "P" ;
    skos:prefLabel "Production based"@en ;
    rdfs:label "Production based"@en .

sip-sdmx-code:demandProd skos:hasTopConcept sip-sdmx-code:demandProd-P .


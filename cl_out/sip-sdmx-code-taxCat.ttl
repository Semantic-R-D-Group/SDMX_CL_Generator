@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sdmx: <http://purl.org/linked-data/sdmx#> .
@prefix sdmx-concept: <http://purl.org/linked-data/sdmx/2009/concept#> .
@prefix sdmx-code: <http://purl.org/linked-data/sdmx/2009/code#> .
@prefix sip-sdmx-concept: <https://purl.semanticip.org/linked-data/sdmx/concept/> .
@prefix sip-sdmx-code: <https://purl.semanticip.org/linked-data/sdmx/code/> .
@prefix sip-sdmx: <https://purl.semanticip.org/linked-data/sdmx> .


#####################################################
# CL_TAXCAT Scheme Definition
#####################################################

sip-sdmx-code:taxCat a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "GFS tax category - codelist scheme"@en ;
    rdfs:label "GFS tax category - codelist scheme"@en ;
    skos:notation "CL_TAXCAT" ;
    skos:definition "A reference taxCat codelist based on ESTAT:CL_GFS_TAXCAT(1.2)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/ESTAT/CL_GFS_TAXCAT/1.2> ;
    rdfs:seeAlso sip-sdmx-code:TaxCat .

sip-sdmx-code:TaxCat a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "GFS tax category - codelist class"@en ;
    rdfs:label "GFS tax category - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:taxCat ;
    skos:inScheme sip-sdmx-code:taxCat .


########################################
# CL_TAXCAT Codes
########################################
    
sip-sdmx-code:taxCat-AT a skos:Concept, sip-sdmx-code:TaxCat;
    skos:topConceptOf sip-sdmx-code:taxCat ;
    skos:inScheme sip-sdmx-code:taxCat ;
    skos:notation "AT" ;
    skos:prefLabel "Alcohol and tabacco tax"@en ;
    rdfs:label "Alcohol and tabacco tax"@en .

sip-sdmx-code:taxCat skos:hasTopConcept sip-sdmx-code:taxCat-AT .



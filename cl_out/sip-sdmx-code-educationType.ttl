    skos:notation "FE" ;
    skos:prefLabel "Formal education, includes formal initial and formal adult education programmes"@en ;
    rdfs:label "Formal education, includes formal initial and formal adult education programmes"@en .

sip-sdmx-code:educationType skos:hasTopConcept sip-sdmx-code:educationType-FE .



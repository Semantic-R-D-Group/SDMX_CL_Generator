#####################################################
# CL_EDUCATIONTYPE Scheme Definition
#####################################################

sip-sdmx-code:educationType a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Education type - codelist scheme"@en ;
    rdfs:label "Education type - codelist scheme"@en ;
    skos:notation "CL_EDUCATIONTYPE" ;
    skos:definition "A reference educationType codelist based on UIS:CL_EDUCATION_TYPE(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/UIS/CL_EDUCATION_TYPE/1.0> ;
    rdfs:seeAlso sip-sdmx-code:EducationType .

sip-sdmx-code:EducationType a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Education type - codelist class"@en ;
    rdfs:label "Education type - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:educationType ;
    skos:inScheme sip-sdmx-code:educationType .


########################################
# CL_EDUCATIONTYPE Codes
########################################
    
sip-sdmx-code:educationType-FE a skos:Concept, sip-sdmx-code:EducationType;

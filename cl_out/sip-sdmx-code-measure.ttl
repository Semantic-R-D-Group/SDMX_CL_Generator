#####################################################
# CL_MEASURE Scheme Definition
#####################################################

sip-sdmx-code:measure a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "CL_MEASURE - codelist scheme"@en ;
    rdfs:label "CL_MEASURE - codelist scheme"@en ;
    skos:notation "CL_MEASURE" ;
    skos:definition "A reference measure codelist based on UNSD:CL_MEASURE(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/UNSD/CL_MEASURE/1.0> ;
    rdfs:seeAlso sip-sdmx-code:Measure .

sip-sdmx-code:Measure a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "CL_MEASURE - codelist class"@en ;
    rdfs:label "CL_MEASURE - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:measure ;
    skos:inScheme sip-sdmx-code:measure .


########################################
# CL_MEASURE Codes
########################################
    
sip-sdmx-code:measure-PI a skos:Concept, sip-sdmx-code:Measure;
    skos:topConceptOf sip-sdmx-code:measure ;
    skos:inScheme sip-sdmx-code:measure ;
    skos:notation "PI" ;
    skos:prefLabel "Price index"@en ;
    rdfs:label "Price index"@en .

sip-sdmx-code:measure skos:hasTopConcept sip-sdmx-code:measure-PI .



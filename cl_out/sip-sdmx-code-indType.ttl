#####################################################
# CL_INDTYPE Scheme Definition
#####################################################

sip-sdmx-code:indType a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Indicator type - codelist scheme"@en ;
    rdfs:label "Indicator type - codelist scheme"@en ;
    skos:notation "CL_INDTYPE" ;
    skos:definition "A reference indType codelist based on ESTAT:CL_IND_TYPE(1.1)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/ESTAT/CL_IND_TYPE/1.1> ;
    rdfs:seeAlso sip-sdmx-code:IndType .

sip-sdmx-code:IndType a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Indicator type - codelist class"@en ;
    rdfs:label "Indicator type - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:indType ;
    skos:inScheme sip-sdmx-code:indType .



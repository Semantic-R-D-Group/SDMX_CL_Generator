#####################################################
# CL_VALUATION Scheme Definition
#####################################################

sip-sdmx-code:valuation a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Valuation - codelist scheme"@en ;
    rdfs:label "Valuation - codelist scheme"@en ;
    skos:notation "CL_VALUATION" ;
    skos:definition "A reference valuation codelist based on ESTAT:CL_VALUATION(1.6)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/ESTAT/CL_VALUATION/1.6> ;
    rdfs:seeAlso sip-sdmx-concept:VALUATION ;
    skos:related sip-sdmx-concept:VALUATION ;
    rdfs:seeAlso sip-sdmx-code:Valuation .

sip-sdmx-code:Valuation a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Valuation - codelist class"@en ;
    rdfs:label "Valuation - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:valuation ;
    skos:inScheme sip-sdmx-code:valuation .



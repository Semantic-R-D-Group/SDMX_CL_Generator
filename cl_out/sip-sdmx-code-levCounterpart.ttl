@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sdmx: <http://purl.org/linked-data/sdmx#> .
@prefix sdmx-concept: <http://purl.org/linked-data/sdmx/2009/concept#> .
@prefix sdmx-code: <http://purl.org/linked-data/sdmx/2009/code#> .
@prefix sip-sdmx-concept: <https://purl.semanticip.org/linked-data/sdmx/concept/> .
@prefix sip-sdmx-code: <https://purl.semanticip.org/linked-data/sdmx/code/> .
@prefix sip-sdmx: <https://purl.semanticip.org/linked-data/sdmx> .


#####################################################
# CL_LEVCOUNTERPART Scheme Definition
#####################################################

sip-sdmx-code:levCounterpart a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Level of counterpart code list - codelist scheme"@en ;
    rdfs:label "Level of counterpart code list - codelist scheme"@en ;
    skos:notation "CL_LEVCOUNTERPART" ;
    skos:definition "A reference levCounterpart codelist based on OECD:CL_LEV_COUNTERPART(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/OECD/CL_LEV_COUNTERPART/1.0> ;
    rdfs:seeAlso sip-sdmx-code:LevCounterpart .

sip-sdmx-code:LevCounterpart a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Level of counterpart code list - codelist class"@en ;
    rdfs:label "Level of counterpart code list - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:levCounterpart ;
    skos:inScheme sip-sdmx-code:levCounterpart .


########################################
# CL_LEVCOUNTERPART Codes
########################################
    
sip-sdmx-code:levCounterpart-IMC a skos:Concept, sip-sdmx-code:LevCounterpart;
    skos:topConceptOf sip-sdmx-code:levCounterpart ;
    skos:inScheme sip-sdmx-code:levCounterpart ;
    skos:notation "IMC" ;
    skos:prefLabel "Immediate counterpart (Immediate investor or immediate host)"@en ;
    rdfs:label "Immediate counterpart (Immediate investor or immediate host)"@en .

sip-sdmx-code:levCounterpart skos:hasTopConcept sip-sdmx-code:levCounterpart-IMC .


sip-sdmx-code:levCounterpart-LSP a skos:Concept, sip-sdmx-code:LevCounterpart;
    skos:topConceptOf sip-sdmx-code:levCounterpart ;
    skos:inScheme sip-sdmx-code:levCounterpart ;
    skos:notation "LSP" ;
    skos:prefLabel "Counterpart looking through non-resident SPEs (non-resident Operating counterpart)"@en ;
    rdfs:label "Counterpart looking through non-resident SPEs (non-resident Operating counterpart)"@en .

sip-sdmx-code:levCounterpart skos:hasTopConcept sip-sdmx-code:levCounterpart-LSP .


sip-sdmx-code:levCounterpart-ULT a skos:Concept, sip-sdmx-code:LevCounterpart;
    skos:topConceptOf sip-sdmx-code:levCounterpart ;
    skos:inScheme sip-sdmx-code:levCounterpart ;
    skos:notation "ULT" ;
    skos:prefLabel "Ultimate counterpart (Ultimate investor or ultimate host)"@en ;
    rdfs:label "Ultimate counterpart (Ultimate investor or ultimate host)"@en .

sip-sdmx-code:levCounterpart skos:hasTopConcept sip-sdmx-code:levCounterpart-ULT .


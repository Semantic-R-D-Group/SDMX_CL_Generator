#####################################################
# CL_BREAKDOWNGROUP Scheme Definition
#####################################################

sip-sdmx-code:breakdownGroup a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Breakdown group - codelist scheme"@en ;
    rdfs:label "Breakdown group - codelist scheme"@en ;
    skos:notation "CL_BREAKDOWNGROUP" ;
    skos:definition "A reference breakdownGroup codelist based on UIS:CL_BREAKDOWN_GROUP(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/UIS/CL_BREAKDOWN_GROUP/1.0> ;
    rdfs:seeAlso sip-sdmx-code:BreakdownGroup .

sip-sdmx-code:BreakdownGroup a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Breakdown group - codelist class"@en ;
    rdfs:label "Breakdown group - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:breakdownGroup ;
    skos:inScheme sip-sdmx-code:breakdownGroup .



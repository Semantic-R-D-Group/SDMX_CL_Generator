#####################################################
# CL_REFPERIOD Scheme Definition
#####################################################

sip-sdmx-code:refPeriod a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Reference period detail codes - codelist scheme"@en ;
    rdfs:label "Reference period detail codes - codelist scheme"@en ;
    skos:notation "CL_REFPERIOD" ;
    skos:definition "A reference refPeriod codelist based on ESTAT:CL_REF_PERIOD_DTL(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/ESTAT/CL_REF_PERIOD_DTL/1.0> ;
    rdfs:seeAlso sip-sdmx-code:RefPeriod .

sip-sdmx-code:RefPeriod a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Reference period detail codes - codelist class"@en ;
    rdfs:label "Reference period detail codes - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:refPeriod ;
    skos:inScheme sip-sdmx-code:refPeriod .



#####################################################
# CL_URBANISATION Scheme Definition
#####################################################

sip-sdmx-code:urbanisation a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Code list for degree of urbanisation - codelist scheme"@en ;
    rdfs:label "Code list for degree of urbanisation - codelist scheme"@en ;
    skos:altLabel "Degree of urbanisation code list - codelist scheme"@en ;
    skos:notation "CL_URBANISATION" ;
    skos:definition "A reference urbanisation codelist combining IAEG-SDGs:CL_URBANISATION(1.10), SDMX:CL_DEG_URB(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/IAEG-SDGs/CL_URBANISATION/1.10> ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/SDMX/CL_DEG_URB/1.0> ;
    skos:note "SDMX:CL_DEG_URB(1.0): This code list provides coded information about the degree to which area is urbanised."@en ;
    rdfs:seeAlso sip-sdmx-code:Urbanisation .

sip-sdmx-code:Urbanisation a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Code list for degree of urbanisation - codelist class"@en ;
    rdfs:label "Code list for degree of urbanisation - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:urbanisation ;
    skos:inScheme sip-sdmx-code:urbanisation .



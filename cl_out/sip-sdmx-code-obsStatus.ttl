#####################################################
# CL_OBSSTATUS Scheme Definition
#####################################################

sip-sdmx-code:obsStatus a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Observation status - codelist scheme"@en ;
    rdfs:label "Observation status - codelist scheme"@en ;
    skos:altLabel "Observation Status - codelist scheme"@en ;
    skos:notation "CL_OBSSTATUS" ;
    skos:definition "A reference obsStatus codelist combining ESTAT:CL_OBS_STATUS(2.3), SDMX:CL_OBS_STATUS(2.2)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/ESTAT/CL_OBS_STATUS/2.3> ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/SDMX/CL_OBS_STATUS/2.2> ;
    rdfs:seeAlso sip-sdmx-concept:OBS_STATUS ;
    skos:related sip-sdmx-concept:OBS_STATUS ;
    rdfs:seeAlso sip-sdmx-code:ObsStatus .

sip-sdmx-code:obsStatus skos:exactMatch sdmx-code:obsStatus .

sip-sdmx-code:ObsStatus a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Observation status - codelist class"@en ;
    rdfs:label "Observation status - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:obsStatus ;
    skos:inScheme sip-sdmx-code:obsStatus .



#####################################################
# CL_WORKBALANCE Scheme Definition
#####################################################

sip-sdmx-code:workBalance a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "EDP working balance basis - codelist scheme"@en ;
    rdfs:label "EDP working balance basis - codelist scheme"@en ;
    skos:notation "CL_WORKBALANCE" ;
    skos:definition "A reference workBalance codelist based on ESTAT:CL_EDP_WBB(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/ESTAT/CL_EDP_WBB/1.0> ;
    rdfs:seeAlso sip-sdmx-code:WorkBalance .

sip-sdmx-code:WorkBalance a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "EDP working balance basis - codelist class"@en ;
    rdfs:label "EDP working balance basis - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:workBalance ;
    skos:inScheme sip-sdmx-code:workBalance .


########################################
# CL_WORKBALANCE Codes
########################################
    
sip-sdmx-code:workBalance-A a skos:Concept, sip-sdmx-code:WorkBalance;
    skos:topConceptOf sip-sdmx-code:workBalance ;
    skos:inScheme sip-sdmx-code:workBalance ;
    skos:notation "A" ;
    skos:prefLabel "Accrual"@en ;
    rdfs:label "Accrual"@en .

sip-sdmx-code:workBalance skos:hasTopConcept sip-sdmx-code:workBalance-A .


sip-sdmx-code:workBalance-C a skos:Concept, sip-sdmx-code:WorkBalance;
    skos:topConceptOf sip-sdmx-code:workBalance ;
    skos:inScheme sip-sdmx-code:workBalance ;
    skos:notation "C" ;
    skos:prefLabel "Cash"@en ;
    rdfs:label "Cash"@en .

sip-sdmx-code:workBalance skos:hasTopConcept sip-sdmx-code:workBalance-C .


sip-sdmx-code:workBalance-M a skos:Concept, sip-sdmx-code:WorkBalance;
    skos:topConceptOf sip-sdmx-code:workBalance ;
    skos:inScheme sip-sdmx-code:workBalance ;
    skos:notation "M" ;
    skos:prefLabel "Mixed"@en ;
    rdfs:label "Mixed"@en .

sip-sdmx-code:workBalance skos:hasTopConcept sip-sdmx-code:workBalance-M .


sip-sdmx-code:workBalance-_O a skos:Concept, sip-sdmx-code:WorkBalance;
    skos:topConceptOf sip-sdmx-code:workBalance ;
    skos:inScheme sip-sdmx-code:workBalance ;
    skos:notation "_O" ;
    skos:prefLabel "Other"@en ;
    rdfs:label "Other"@en .

sip-sdmx-code:workBalance skos:hasTopConcept sip-sdmx-code:workBalance-_O .


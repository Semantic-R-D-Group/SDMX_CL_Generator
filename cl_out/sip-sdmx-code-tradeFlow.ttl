#####################################################
# CL_TRADEFLOW Scheme Definition
#####################################################

sip-sdmx-code:tradeFlow a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "CL_TRADE_FLOW - codelist scheme"@en ;
    rdfs:label "CL_TRADE_FLOW - codelist scheme"@en ;
    skos:notation "CL_TRADEFLOW" ;
    skos:definition "A reference tradeFlow codelist based on UNSD:CL_TRADE_FLOW(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/UNSD/CL_TRADE_FLOW/1.0> ;
    rdfs:seeAlso sip-sdmx-code:TradeFlow .

sip-sdmx-code:TradeFlow a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "CL_TRADE_FLOW - codelist class"@en ;
    rdfs:label "CL_TRADE_FLOW - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:tradeFlow ;
    skos:inScheme sip-sdmx-code:tradeFlow .



#####################################################
# CL_STATUNIT Scheme Definition
#####################################################

sip-sdmx-code:statUnit a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Statistical unit - codelist scheme"@en ;
    rdfs:label "Statistical unit - codelist scheme"@en ;
    skos:altLabel "Statistical unit code list - codelist scheme"@en ;
    skos:notation "CL_STATUNIT" ;
    skos:definition "A reference statUnit codelist combining OECD:CL_STAT_UNIT(1.0), UIS:CL_STAT_UNIT(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/OECD/CL_STAT_UNIT/1.0> ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/UIS/CL_STAT_UNIT/1.0> ;
    rdfs:seeAlso sip-sdmx-code:StatUnit .

sip-sdmx-code:StatUnit a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Statistical unit - codelist class"@en ;
    rdfs:label "Statistical unit - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:statUnit ;
    skos:inScheme sip-sdmx-code:statUnit .



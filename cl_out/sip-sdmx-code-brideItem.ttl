#####################################################
# CL_BRIDEITEM Scheme Definition
#####################################################

sip-sdmx-code:brideItem a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Bridging items - codelist scheme"@en ;
    rdfs:label "Bridging items - codelist scheme"@en ;
    skos:notation "CL_BRIDEITEM" ;
    skos:definition "A reference brideItem codelist based on ESTAT:CL_BRIDGE_ITEM(1.3.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/ESTAT/CL_BRIDGE_ITEM/1.3.0> ;
    skos:note "ESTAT:CL_BRIDGE_ITEM(1.3.0): Items used to bridge for territorial coverage"@en ;
    rdfs:seeAlso sip-sdmx-code:BrideItem .

sip-sdmx-code:BrideItem a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Bridging items - codelist class"@en ;
    rdfs:label "Bridging items - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:brideItem ;
    skos:inScheme sip-sdmx-code:brideItem .



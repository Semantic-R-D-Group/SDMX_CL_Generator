#####################################################
# CL_TIMEFORMAT Scheme Definition
#####################################################

sip-sdmx-code:timeFormat a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Time Format - codelist scheme"@en ;
    rdfs:label "Time Format - codelist scheme"@en ;
    skos:notation "CL_TIMEFORMAT" ;
    skos:definition "A reference timeFormat codelist based on SDMX:CL_TIME_FORMAT(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/SDMX/CL_TIME_FORMAT/1.0> ;
    skos:note "SDMX:CL_TIME_FORMAT(1.0): This code list provides coded information (based on the ISO 8601 standard) indicating the type of time references used in the data. It was released in 2009. More information about this code list and SDMX code lists in general (e.g. list of generic codes for expressing general concepts like \"Total\", \"Unknown\", etc.; syntaxes for the creation of further codes; general guidelines for the creation of SDMX code lists) can be found at this address: https://sdmx.org/?page_id=4345."@en ;
    rdfs:seeAlso sip-sdmx-concept:TIME_FORMAT ;
    skos:related sip-sdmx-concept:TIME_FORMAT ;
    rdfs:seeAlso sip-sdmx-code:TimeFormat .

sip-sdmx-code:timeFormat skos:exactMatch sdmx-code:timeFormat .

sip-sdmx-code:TimeFormat a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Time Format - codelist class"@en ;
    rdfs:label "Time Format - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:timeFormat ;
    skos:inScheme sip-sdmx-code:timeFormat .



#####################################################
# CL_TYPEENTITY Scheme Definition
#####################################################

sip-sdmx-code:typeEntity a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Type of entity code list - codelist scheme"@en ;
    rdfs:label "Type of entity code list - codelist scheme"@en ;
    skos:notation "CL_TYPEENTITY" ;
    skos:definition "A reference typeEntity codelist based on OECD:CL_TYPE_ENTITY(1.1)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/OECD/CL_TYPE_ENTITY/1.1> ;
    rdfs:seeAlso sip-sdmx-code:TypeEntity .

sip-sdmx-code:TypeEntity a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Type of entity code list - codelist class"@en ;
    rdfs:label "Type of entity code list - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:typeEntity ;
    skos:inScheme sip-sdmx-code:typeEntity .


########################################
# CL_TYPEENTITY Codes
########################################
    
sip-sdmx-code:typeEntity-ALL a skos:Concept, sip-sdmx-code:TypeEntity;
    skos:topConceptOf sip-sdmx-code:typeEntity ;
    skos:inScheme sip-sdmx-code:typeEntity ;
    skos:notation "ALL" ;
    skos:prefLabel "All resident units"@en ;
    rdfs:label "All resident units"@en .

sip-sdmx-code:typeEntity skos:hasTopConcept sip-sdmx-code:typeEntity-ALL .



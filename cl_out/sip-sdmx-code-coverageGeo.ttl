#####################################################
# CL_COVERAGEGEO Scheme Definition
#####################################################

sip-sdmx-code:coverageGeo a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Geographical coverage - codelist scheme"@en ;
    rdfs:label "Geographical coverage - codelist scheme"@en ;
    skos:notation "CL_COVERAGEGEO" ;
    skos:definition "A reference coverageGeo codelist based on ESTAT:CL_COVERAGE_GEO(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/ESTAT/CL_COVERAGE_GEO/1.0> ;
    skos:note "ESTAT:CL_COVERAGE_GEO(1.0): Defines which type of region is used to compile the index"@en ;
    rdfs:seeAlso sip-sdmx-code:CoverageGeo .

sip-sdmx-code:CoverageGeo a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Geographical coverage - codelist class"@en ;
    rdfs:label "Geographical coverage - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:coverageGeo ;
    skos:inScheme sip-sdmx-code:coverageGeo .



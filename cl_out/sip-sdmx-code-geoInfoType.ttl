@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sdmx: <http://purl.org/linked-data/sdmx#> .
@prefix sdmx-concept: <http://purl.org/linked-data/sdmx/2009/concept#> .
@prefix sdmx-code: <http://purl.org/linked-data/sdmx/2009/code#> .
@prefix sip-sdmx-concept: <https://purl.semanticip.org/linked-data/sdmx/concept/> .
@prefix sip-sdmx-code: <https://purl.semanticip.org/linked-data/sdmx/code/> .
@prefix sip-sdmx: <https://purl.semanticip.org/linked-data/sdmx> .


#####################################################
# CL_GEOINFOTYPE Scheme Definition
#####################################################

sip-sdmx-code:geoInfoType a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Geoinformation type code list - codelist scheme"@en ;
    rdfs:label "Geoinformation type code list - codelist scheme"@en ;
    skos:notation "CL_GEOINFOTYPE" ;
    skos:definition "A reference geoInfoType codelist based on IAEG-SDGs:CL_GEO_INFO_TYPE(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/IAEG-SDGs/CL_GEO_INFO_TYPE/1.0> ;
    rdfs:seeAlso sip-sdmx-code:GeoInfoType .

sip-sdmx-code:GeoInfoType a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Geoinformation type code list - codelist class"@en ;
    rdfs:label "Geoinformation type code list - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:geoInfoType ;
    skos:inScheme sip-sdmx-code:geoInfoType .


########################################
# CL_GEOINFOTYPE Codes
########################################
    
sip-sdmx-code:geoInfoType-GML a skos:Concept, sip-sdmx-code:GeoInfoType;
    skos:topConceptOf sip-sdmx-code:geoInfoType ;
    skos:inScheme sip-sdmx-code:geoInfoType ;
    skos:notation "GML" ;
    skos:prefLabel "Geography Markup Language"@en ;
    rdfs:label "Geography Markup Language"@en .

sip-sdmx-code:geoInfoType skos:hasTopConcept sip-sdmx-code:geoInfoType-GML .


sip-sdmx-code:geoInfoType-GMLJP2 a skos:Concept, sip-sdmx-code:GeoInfoType;
    skos:topConceptOf sip-sdmx-code:geoInfoType ;
    skos:inScheme sip-sdmx-code:geoInfoType ;
    skos:notation "GMLJP2" ;
    skos:prefLabel "GML in JPEG 2000 for Geographic Imagery"@en ;
    rdfs:label "GML in JPEG 2000 for Geographic Imagery"@en .

sip-sdmx-code:geoInfoType skos:hasTopConcept sip-sdmx-code:geoInfoType-GMLJP2 .


sip-sdmx-code:geoInfoType-GeoJSON_1_1 a skos:Concept, sip-sdmx-code:GeoInfoType;
    skos:topConceptOf sip-sdmx-code:geoInfoType ;
    skos:inScheme sip-sdmx-code:geoInfoType ;
    skos:notation "GeoJSON_1_1" ;
    skos:prefLabel "GeoJSON version 1.0"@en ;
    rdfs:label "GeoJSON version 1.0"@en .

sip-sdmx-code:geoInfoType skos:hasTopConcept sip-sdmx-code:geoInfoType-GeoJSON_1_1 .


sip-sdmx-code:geoInfoType-KML_2_2 a skos:Concept, sip-sdmx-code:GeoInfoType;
    skos:topConceptOf sip-sdmx-code:geoInfoType ;
    skos:inScheme sip-sdmx-code:geoInfoType ;
    skos:notation "KML_2_2" ;
    skos:prefLabel "KML version 2.2"@en ;
    rdfs:label "KML version 2.2"@en .

sip-sdmx-code:geoInfoType skos:hasTopConcept sip-sdmx-code:geoInfoType-KML_2_2 .


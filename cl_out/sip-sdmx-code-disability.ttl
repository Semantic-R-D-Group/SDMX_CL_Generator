@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sdmx: <http://purl.org/linked-data/sdmx#> .
@prefix sdmx-concept: <http://purl.org/linked-data/sdmx/2009/concept#> .
@prefix sdmx-code: <http://purl.org/linked-data/sdmx/2009/code#> .
@prefix sip-sdmx-concept: <https://purl.semanticip.org/linked-data/sdmx/concept/> .
@prefix sip-sdmx-code: <https://purl.semanticip.org/linked-data/sdmx/code/> .
@prefix sip-sdmx: <https://purl.semanticip.org/linked-data/sdmx> .


#####################################################
# CL_DISABILITY Scheme Definition
#####################################################

sip-sdmx-code:disability a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Disability status code list - codelist scheme"@en ;
    rdfs:label "Disability status code list - codelist scheme"@en ;
    skos:notation "CL_DISABILITY" ;
    skos:definition "A reference disability codelist based on IAEG-SDGs:CL_DISABILITY(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/IAEG-SDGs/CL_DISABILITY/1.0> ;
    rdfs:seeAlso sip-sdmx-code:Disability .

sip-sdmx-code:Disability a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Disability status code list - codelist class"@en ;
    rdfs:label "Disability status code list - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:disability ;
    skos:inScheme sip-sdmx-code:disability .


########################################
# CL_DISABILITY Codes
########################################
    
sip-sdmx-code:disability-PD a skos:Concept, sip-sdmx-code:Disability;
    skos:topConceptOf sip-sdmx-code:disability ;
    skos:inScheme sip-sdmx-code:disability ;
    skos:notation "PD" ;
    skos:prefLabel "Persons with disability"@en ;
    rdfs:label "Persons with disability"@en .

sip-sdmx-code:disability skos:hasTopConcept sip-sdmx-code:disability-PD .


sip-sdmx-code:disability-PWD a skos:Concept, sip-sdmx-code:Disability;
    skos:topConceptOf sip-sdmx-code:disability ;
    skos:inScheme sip-sdmx-code:disability ;
    skos:notation "PWD" ;
    skos:prefLabel "Persons without disability"@en ;
    rdfs:label "Persons without disability"@en .

sip-sdmx-code:disability skos:hasTopConcept sip-sdmx-code:disability-PWD .


sip-sdmx-code:disability-_T a skos:Concept, sip-sdmx-code:Disability;
    skos:topConceptOf sip-sdmx-code:disability ;
    skos:inScheme sip-sdmx-code:disability ;
    skos:notation "_T" ;
    skos:prefLabel "No breakdown"@en ;
    rdfs:label "No breakdown"@en .

sip-sdmx-code:disability skos:hasTopConcept sip-sdmx-code:disability-_T .


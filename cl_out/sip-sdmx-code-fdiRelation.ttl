@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sdmx: <http://purl.org/linked-data/sdmx#> .
@prefix sdmx-concept: <http://purl.org/linked-data/sdmx/2009/concept#> .
@prefix sdmx-code: <http://purl.org/linked-data/sdmx/2009/code#> .
@prefix sip-sdmx-concept: <https://purl.semanticip.org/linked-data/sdmx/concept/> .
@prefix sip-sdmx-code: <https://purl.semanticip.org/linked-data/sdmx/code/> .
@prefix sip-sdmx: <https://purl.semanticip.org/linked-data/sdmx> .


#####################################################
# CL_FDIRELATION Scheme Definition
#####################################################

sip-sdmx-code:fdiRelation a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "FDI relationship code list - codelist scheme"@en ;
    rdfs:label "FDI relationship code list - codelist scheme"@en ;
    skos:notation "CL_FDIRELATION" ;
    skos:definition "A reference fdiRelation codelist based on OECD:CL_FDI_RELATION(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/OECD/CL_FDI_RELATION/1.0> ;
    rdfs:seeAlso sip-sdmx-code:FdiRelation .

sip-sdmx-code:FdiRelation a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "FDI relationship code list - codelist class"@en ;
    rdfs:label "FDI relationship code list - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:fdiRelation ;
    skos:inScheme sip-sdmx-code:fdiRelation .


########################################
# CL_FDIRELATION Codes
########################################
    
sip-sdmx-code:fdiRelation-C a skos:Concept, sip-sdmx-code:FdiRelation;
    skos:topConceptOf sip-sdmx-code:fdiRelation ;
    skos:inScheme sip-sdmx-code:fdiRelation ;
    skos:notation "C" ;
    skos:prefLabel "Control >50% of voting power"@en ;
    rdfs:label "Control >50% of voting power"@en .

sip-sdmx-code:fdiRelation skos:hasTopConcept sip-sdmx-code:fdiRelation-C .


sip-sdmx-code:fdiRelation-I a skos:Concept, sip-sdmx-code:FdiRelation;
    skos:topConceptOf sip-sdmx-code:fdiRelation ;
    skos:inScheme sip-sdmx-code:fdiRelation ;
    skos:notation "I" ;
    skos:prefLabel "Influence >=10% of voting power"@en ;
    rdfs:label "Influence >=10% of voting power"@en .

sip-sdmx-code:fdiRelation skos:hasTopConcept sip-sdmx-code:fdiRelation-I .


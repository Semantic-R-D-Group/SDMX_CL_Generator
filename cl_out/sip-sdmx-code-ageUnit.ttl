    skos:notation "M" ;
    skos:prefLabel "Month(s)"@en ;
    rdfs:label "Month(s)"@en .

sip-sdmx-code:ageUnit skos:hasTopConcept sip-sdmx-code:ageUnit-M .


sip-sdmx-code:ageUnit-W a skos:Concept, sip-sdmx-code:AgeUnit;

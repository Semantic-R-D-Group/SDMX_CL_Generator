@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sdmx: <http://purl.org/linked-data/sdmx#> .
@prefix sdmx-concept: <http://purl.org/linked-data/sdmx/2009/concept#> .
@prefix sdmx-code: <http://purl.org/linked-data/sdmx/2009/code#> .
@prefix sip-sdmx-concept: <https://purl.semanticip.org/linked-data/sdmx/concept/> .
@prefix sip-sdmx-code: <https://purl.semanticip.org/linked-data/sdmx/code/> .
@prefix sip-sdmx: <https://purl.semanticip.org/linked-data/sdmx> .


#####################################################
# CL_MOBILITY Scheme Definition
#####################################################

sip-sdmx-code:mobility a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Mobility - codelist scheme"@en ;
    rdfs:label "Mobility - codelist scheme"@en ;
    skos:notation "CL_MOBILITY" ;
    skos:definition "A reference mobility codelist based on UIS:CL_MOBILITY(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/UIS/CL_MOBILITY/1.0> ;
    rdfs:seeAlso sip-sdmx-code:Mobility .

sip-sdmx-code:Mobility a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Mobility - codelist class"@en ;
    rdfs:label "Mobility - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:mobility ;
    skos:inScheme sip-sdmx-code:mobility .


########################################
# CL_MOBILITY Codes
########################################
    
sip-sdmx-code:mobility-MOB a skos:Concept, sip-sdmx-code:Mobility;
    skos:topConceptOf sip-sdmx-code:mobility ;
    skos:inScheme sip-sdmx-code:mobility ;
    skos:notation "MOB" ;
    skos:prefLabel "Mobile including homecoming nationals"@en ;
    rdfs:label "Mobile including homecoming nationals"@en .

sip-sdmx-code:mobility skos:hasTopConcept sip-sdmx-code:mobility-MOB .


sip-sdmx-code:mobility-MOB_HOME a skos:Concept, sip-sdmx-code:Mobility;

#####################################################
# CL_SECTOR Scheme Definition
#####################################################

sip-sdmx-code:sector a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Reference/Counterpart institutional sector - codelist scheme"@en ;
    rdfs:label "Reference/Counterpart institutional sector - codelist scheme"@en ;
    skos:notation "CL_SECTOR" ;
    skos:definition "A reference sector codelist based on UIS:CL_SECTOR(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/UIS/CL_SECTOR/1.0> ;
    rdfs:seeAlso sip-sdmx-code:Sector .

sip-sdmx-code:Sector a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Reference/Counterpart institutional sector - codelist class"@en ;
    rdfs:label "Reference/Counterpart institutional sector - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:sector ;
    skos:inScheme sip-sdmx-code:sector .



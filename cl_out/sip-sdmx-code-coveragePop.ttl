#####################################################
# CL_COVERAGEPOP Scheme Definition
#####################################################

sip-sdmx-code:coveragePop a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Population coverage - codelist scheme"@en ;
    rdfs:label "Population coverage - codelist scheme"@en ;
    skos:notation "CL_COVERAGEPOP" ;
    skos:definition "A reference coveragePop codelist based on ESTAT:CL_COVERAGE_POP(1.1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/ESTAT/CL_COVERAGE_POP/1.1.0> ;
    rdfs:seeAlso sip-sdmx-code:CoveragePop .

sip-sdmx-code:CoveragePop a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Population coverage - codelist class"@en ;
    rdfs:label "Population coverage - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:coveragePop ;
    skos:inScheme sip-sdmx-code:coveragePop .



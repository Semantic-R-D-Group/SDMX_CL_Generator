#####################################################
# CL_ECONOMICFUNC Scheme Definition
#####################################################

sip-sdmx-code:economicFunc a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "GFS economic function - codelist scheme"@en ;
    rdfs:label "GFS economic function - codelist scheme"@en ;
    skos:notation "CL_ECONOMICFUNC" ;
    skos:definition "A reference economicFunc codelist based on ESTAT:CL_GFS_ECOFUNC(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/ESTAT/CL_GFS_ECOFUNC/1.0> ;
    rdfs:seeAlso sip-sdmx-code:EconomicFunc .

sip-sdmx-code:EconomicFunc a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "GFS economic function - codelist class"@en ;
    rdfs:label "GFS economic function - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:economicFunc ;
    skos:inScheme sip-sdmx-code:economicFunc .



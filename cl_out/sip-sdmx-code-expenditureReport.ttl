#####################################################
# CL_EXPENDITUREREPORT Scheme Definition
#####################################################

sip-sdmx-code:expenditureReport a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Type of expenditure reported - codelist scheme"@en ;
    rdfs:label "Type of expenditure reported - codelist scheme"@en ;
    skos:notation "CL_EXPENDITUREREPORT" ;
    skos:definition "A reference expenditureReport codelist based on UIS:CL_TYPE_EXP_REPORTED(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/UIS/CL_TYPE_EXP_REPORTED/1.0> ;
    rdfs:seeAlso sip-sdmx-code:ExpenditureReport .

sip-sdmx-code:ExpenditureReport a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Type of expenditure reported - codelist class"@en ;
    rdfs:label "Type of expenditure reported - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:expenditureReport ;
    skos:inScheme sip-sdmx-code:expenditureReport .


########################################
# CL_EXPENDITUREREPORT Codes
########################################
    
sip-sdmx-code:expenditureReport-ACTUAL a skos:Concept, sip-sdmx-code:ExpenditureReport;
    skos:topConceptOf sip-sdmx-code:expenditureReport ;
    skos:inScheme sip-sdmx-code:expenditureReport ;
    skos:notation "ACTUAL" ;
    skos:prefLabel "Actual expenditure"@en ;
    rdfs:label "Actual expenditure"@en .

sip-sdmx-code:expenditureReport skos:hasTopConcept sip-sdmx-code:expenditureReport-ACTUAL .


sip-sdmx-code:expenditureReport-BUDGET a skos:Concept, sip-sdmx-code:ExpenditureReport;

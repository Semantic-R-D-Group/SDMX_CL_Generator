#####################################################
# CL_TRANSPORTMODE Scheme Definition
#####################################################

sip-sdmx-code:transportMode a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "CL_TRANSPORT_MODE - codelist scheme"@en ;
    rdfs:label "CL_TRANSPORT_MODE - codelist scheme"@en ;
    skos:notation "CL_TRANSPORTMODE" ;
    skos:definition "A reference transportMode codelist based on UNSD:CL_TRANSPORT_MODE(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/UNSD/CL_TRANSPORT_MODE/1.0> ;
    rdfs:seeAlso sip-sdmx-code:TransportMode .

sip-sdmx-code:TransportMode a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "CL_TRANSPORT_MODE - codelist class"@en ;
    rdfs:label "CL_TRANSPORT_MODE - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:transportMode ;
    skos:inScheme sip-sdmx-code:transportMode .



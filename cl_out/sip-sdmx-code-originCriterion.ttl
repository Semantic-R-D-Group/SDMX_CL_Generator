    skos:inScheme sip-sdmx-code:originCriterion ;
    skos:notation "OTH" ;
    skos:prefLabel "Other criterion (to be specified)"@en ;
    rdfs:label "Other criterion (to be specified)"@en .

sip-sdmx-code:originCriterion skos:hasTopConcept sip-sdmx-code:originCriterion-OTH .



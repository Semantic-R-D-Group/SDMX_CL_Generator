#####################################################
# CL_ORIGINCRITERION Scheme Definition
#####################################################

sip-sdmx-code:originCriterion a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Origin criterion - codelist scheme"@en ;
    rdfs:label "Origin criterion - codelist scheme"@en ;
    skos:notation "CL_ORIGINCRITERION" ;
    skos:definition "A reference originCriterion codelist based on UIS:CL_ORIGIN_CRITERION(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/UIS/CL_ORIGIN_CRITERION/1.0> ;
    rdfs:seeAlso sip-sdmx-code:OriginCriterion .

sip-sdmx-code:OriginCriterion a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Origin criterion - codelist class"@en ;
    rdfs:label "Origin criterion - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:originCriterion ;
    skos:inScheme sip-sdmx-code:originCriterion .


########################################
# CL_ORIGINCRITERION Codes
########################################
    
sip-sdmx-code:originCriterion-CTZ a skos:Concept, sip-sdmx-code:OriginCriterion;
    skos:topConceptOf sip-sdmx-code:originCriterion ;
    skos:inScheme sip-sdmx-code:originCriterion ;
    skos:notation "CTZ" ;
    skos:prefLabel "Country of citizenship"@en ;
    rdfs:label "Country of citizenship"@en .

sip-sdmx-code:originCriterion skos:hasTopConcept sip-sdmx-code:originCriterion-CTZ .


sip-sdmx-code:originCriterion-OTH a skos:Concept, sip-sdmx-code:OriginCriterion;

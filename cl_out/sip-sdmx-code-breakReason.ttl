#####################################################
# CL_BREAKREASON Scheme Definition
#####################################################

sip-sdmx-code:breakReason a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Reason for the break in time series - codelist scheme"@en ;
    rdfs:label "Reason for the break in time series - codelist scheme"@en ;
    skos:notation "CL_BREAKREASON" ;
    skos:definition "A reference breakReason codelist based on SDMX:CL_BREAK_REASON(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/SDMX/CL_BREAK_REASON/1.0> ;
    rdfs:seeAlso sip-sdmx-code:BreakReason .

sip-sdmx-code:BreakReason a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Reason for the break in time series - codelist class"@en ;
    rdfs:label "Reason for the break in time series - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:breakReason ;
    skos:inScheme sip-sdmx-code:breakReason .



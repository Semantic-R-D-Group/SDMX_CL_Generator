@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sdmx: <http://purl.org/linked-data/sdmx#> .
@prefix sdmx-concept: <http://purl.org/linked-data/sdmx/2009/concept#> .
@prefix sdmx-code: <http://purl.org/linked-data/sdmx/2009/code#> .
@prefix sip-sdmx-concept: <https://purl.semanticip.org/linked-data/sdmx/concept/> .
@prefix sip-sdmx-code: <https://purl.semanticip.org/linked-data/sdmx/code/> .
@prefix sip-sdmx: <https://purl.semanticip.org/linked-data/sdmx> .


#####################################################
# CL_REPORTINGTYPE Scheme Definition
#####################################################

sip-sdmx-code:reportingType a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Reporting type code list - codelist scheme"@en ;
    rdfs:label "Reporting type code list - codelist scheme"@en ;
    skos:notation "CL_REPORTINGTYPE" ;
    skos:definition "A reference reportingType codelist based on IAEG-SDGs:CL_REPORTING_TYPE(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/IAEG-SDGs/CL_REPORTING_TYPE/1.0> ;
    skos:note "IAEG-SDGs:CL_REPORTING_TYPE(1.0): Reporting type code list"@en ;
    rdfs:seeAlso sip-sdmx-code:ReportingType .

sip-sdmx-code:ReportingType a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Reporting type code list - codelist class"@en ;
    rdfs:label "Reporting type code list - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:reportingType ;
    skos:inScheme sip-sdmx-code:reportingType .


########################################
# CL_REPORTINGTYPE Codes
########################################
    
sip-sdmx-code:reportingType-G a skos:Concept, sip-sdmx-code:ReportingType;
    skos:topConceptOf sip-sdmx-code:reportingType ;
    skos:inScheme sip-sdmx-code:reportingType ;
    skos:notation "G" ;
    skos:prefLabel "Global"@en ;
    rdfs:label "Global"@en .

sip-sdmx-code:reportingType skos:hasTopConcept sip-sdmx-code:reportingType-G .


sip-sdmx-code:reportingType-N a skos:Concept, sip-sdmx-code:ReportingType;
    skos:topConceptOf sip-sdmx-code:reportingType ;
    skos:inScheme sip-sdmx-code:reportingType ;
    skos:notation "N" ;
    skos:prefLabel "National"@en ;
    rdfs:label "National"@en .

sip-sdmx-code:reportingType skos:hasTopConcept sip-sdmx-code:reportingType-N .


sip-sdmx-code:reportingType-R a skos:Concept, sip-sdmx-code:ReportingType;
    skos:topConceptOf sip-sdmx-code:reportingType ;
    skos:inScheme sip-sdmx-code:reportingType ;
    skos:notation "R" ;
    skos:prefLabel "Regional"@en ;
    rdfs:label "Regional"@en .

sip-sdmx-code:reportingType skos:hasTopConcept sip-sdmx-code:reportingType-R .


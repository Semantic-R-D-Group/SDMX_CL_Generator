#####################################################
# CL_EXPENDITURETYPE Scheme Definition
#####################################################

sip-sdmx-code:expenditureType a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Expenditure type - codelist scheme"@en ;
    rdfs:label "Expenditure type - codelist scheme"@en ;
    skos:notation "CL_EXPENDITURETYPE" ;
    skos:definition "A reference expenditureType codelist based on UIS:CL_EXPENDITURE_TYPE(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/UIS/CL_EXPENDITURE_TYPE/1.0> ;
    rdfs:seeAlso sip-sdmx-code:ExpenditureType .

sip-sdmx-code:ExpenditureType a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Expenditure type - codelist class"@en ;
    rdfs:label "Expenditure type - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:expenditureType ;
    skos:inScheme sip-sdmx-code:expenditureType .



    skos:inScheme sip-sdmx-code:sex ;
    skos:notation "_T" ;
    skos:prefLabel "Both sexes or no breakdown by sex"@en ;
    sip-sdmx:hasAgencyLabel [sip-sdmx-agency:agenciesID sip-sdmx-agency:IAEG-SDGs ; rdfs:label """Both sexes or no breakdown by sex"""@en ],
                      [sip-sdmx-agency:agenciesID sip-sdmx-agency:SDMX ; rdfs:label """Total"""@en ] .


#####################################################
# CL_PARTNERTYPE Scheme Definition
#####################################################

sip-sdmx-code:partnerType a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "CL_PARTNER_TYPE - codelist scheme"@en ;
    rdfs:label "CL_PARTNER_TYPE - codelist scheme"@en ;
    skos:notation "CL_PARTNERTYPE" ;
    skos:definition "A reference partnerType codelist based on UNSD:CL_PARTNER_TYPE(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/UNSD/CL_PARTNER_TYPE/1.0> ;
    rdfs:seeAlso sip-sdmx-code:PartnerType .

sip-sdmx-code:PartnerType a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "CL_PARTNER_TYPE - codelist class"@en ;
    rdfs:label "CL_PARTNER_TYPE - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:partnerType ;
    skos:inScheme sip-sdmx-code:partnerType .


########################################
# CL_PARTNERTYPE Codes
########################################
    
sip-sdmx-code:partnerType-P1 a skos:Concept, sip-sdmx-code:PartnerType;
    skos:topConceptOf sip-sdmx-code:partnerType ;
    skos:inScheme sip-sdmx-code:partnerType ;
    skos:notation "P1" ;
    skos:prefLabel "Country of origin (for imports)"@en ;
    rdfs:label "Country of origin (for imports)"@en .

sip-sdmx-code:partnerType skos:hasTopConcept sip-sdmx-code:partnerType-P1 .



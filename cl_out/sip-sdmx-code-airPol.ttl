#####################################################
# CL_AIRPOL Scheme Definition
#####################################################

sip-sdmx-code:airPol a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Air pollutants and greenhouse gasses - codelist scheme"@en ;
    rdfs:label "Air pollutants and greenhouse gasses - codelist scheme"@en ;
    skos:notation "CL_AIRPOL" ;
    skos:definition "A reference airPol codelist based on ESTAT:CL_AIRPOL(1.3.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/ESTAT/CL_AIRPOL/1.3.0> ;
    skos:note "ESTAT:CL_AIRPOL(1.3.0): Air pollutants classification"@en ;
    rdfs:seeAlso sip-sdmx-code:AirPol .

sip-sdmx-code:AirPol a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Air pollutants and greenhouse gasses - codelist class"@en ;
    rdfs:label "Air pollutants and greenhouse gasses - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:airPol ;
    skos:inScheme sip-sdmx-code:airPol .



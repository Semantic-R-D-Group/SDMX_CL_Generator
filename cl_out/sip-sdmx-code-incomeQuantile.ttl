#####################################################
# CL_INCOMEQUANTILE Scheme Definition
#####################################################

sip-sdmx-code:incomeQuantile a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Income/wealth quantile code list - codelist scheme"@en ;
    rdfs:label "Income/wealth quantile code list - codelist scheme"@en ;
    skos:notation "CL_INCOMEQUANTILE" ;
    skos:definition "A reference incomeQuantile codelist based on IAEG-SDGs:CL_QUANTILE(1.1)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/IAEG-SDGs/CL_QUANTILE/1.1> ;
    rdfs:seeAlso sip-sdmx-code:IncomeQuantile .

sip-sdmx-code:IncomeQuantile a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Income/wealth quantile code list - codelist class"@en ;
    rdfs:label "Income/wealth quantile code list - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:incomeQuantile ;
    skos:inScheme sip-sdmx-code:incomeQuantile .



#####################################################
# CL_NACONSOLIDAT Scheme Definition
#####################################################

sip-sdmx-code:naConsolidat a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Consolidation codes - codelist scheme"@en ;
    rdfs:label "Consolidation codes - codelist scheme"@en ;
    skos:notation "CL_NACONSOLIDAT" ;
    skos:definition "A reference naConsolidat codelist based on ESTAT:CL_NA_CONSOLIDAT(1.3)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/ESTAT/CL_NA_CONSOLIDAT/1.3> ;
    rdfs:seeAlso sip-sdmx-code:NaConsolidat .

sip-sdmx-code:NaConsolidat a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Consolidation codes - codelist class"@en ;
    rdfs:label "Consolidation codes - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:naConsolidat ;
    skos:inScheme sip-sdmx-code:naConsolidat .



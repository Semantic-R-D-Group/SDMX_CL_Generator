    rdfs:label "Accounting entry code list - codelist scheme"@en ;
    skos:notation "CL_ACCOUNTENTRY" ;
    skos:definition "A reference accountEntry codelist based on IMF:CL_ACCOUNT_ENTRY(1.5.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/IMF/CL_ACCOUNT_ENTRY/1.5.0> ;
    rdfs:seeAlso sip-sdmx-code:AccountEntry .

sip-sdmx-code:AccountEntry a rdfs:Class ;

    skos:prefLabel "Accounting entry code list - codelist class"@en ;
    rdfs:label "Accounting entry code list - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:accountEntry ;
    skos:inScheme sip-sdmx-code:accountEntry .



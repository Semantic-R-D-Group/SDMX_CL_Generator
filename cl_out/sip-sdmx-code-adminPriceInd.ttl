#####################################################
# CL_ADMINPRICEIND Scheme Definition
#####################################################

sip-sdmx-code:adminPriceInd a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Administered prices indicator - codelist scheme"@en ;
    rdfs:label "Administered prices indicator - codelist scheme"@en ;
    skos:notation "CL_ADMINPRICEIND" ;
    skos:definition "A reference adminPriceInd codelist based on ESTAT:CL_AP_DEFINITION(1.0)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/ESTAT/CL_AP_DEFINITION/1.0> ;
    rdfs:seeAlso sip-sdmx-code:AdminPriceInd .

sip-sdmx-code:AdminPriceInd a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Administered prices indicator - codelist class"@en ;
    rdfs:label "Administered prices indicator - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:adminPriceInd ;
    skos:inScheme sip-sdmx-code:adminPriceInd .



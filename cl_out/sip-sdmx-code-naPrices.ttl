#####################################################
# CL_NAPRICES Scheme Definition
#####################################################

sip-sdmx-code:naPrices a skos:ConceptScheme ;
    rdfs:subClassOf sdmx-code:ConceptScheme ;
    skos:prefLabel "Price codes - codelist scheme"@en ;
    rdfs:label "Price codes - codelist scheme"@en ;
    skos:notation "CL_NAPRICES" ;
    skos:definition "A reference naPrices codelist based on ESTAT:CL_NA_PRICES(1.1)"@en ;
    rdfs:seeAlso <https://registry.sdmx.org/sdmx/v2/structure/codelist/ESTAT/CL_NA_PRICES/1.1> ;
    rdfs:seeAlso sip-sdmx-code:NaPrices .

sip-sdmx-code:NaPrices a rdfs:Class ;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Price codes - codelist class"@en ;
    rdfs:label "Price codes - codelist class"@en ;
    rdfs:isDefinedBy sip-sdmx-code:naPrices ;
    skos:inScheme sip-sdmx-code:naPrices .


